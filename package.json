{
  "name": "motomotaapp-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng serve --configuration production",
    "build-dev": "ng build --configuration development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng build --configuration production --aot && firebase deploy --only hosting:motomota",
    "start-pwa": "ng build --configuration production --aot && http-server -p 8080 -c-1 public",
    "firebase-test": "ng build --configuration production --aot && firebase serve -p 8081"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.17",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.17",
    "@angular/compiler": "^20.3.17",
    "@angular/core": "^20.3.17",
    "@angular/forms": "^20.3.17",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.3.17",
    "@angular/platform-browser-dynamic": "^20.3.17",
    "@angular/router": "^20.3.17",
    "@angular/service-worker": "^20.3.17",
    "crypto-js": "^4.2.0",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.18",
    "@angular/cli": "^20.3.18",
    "@angular/compiler-cli": "^20.3.17",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3"
  }
}